    "strimzi",
    "kafka",
    "kubernetes",
    "operator"
  ],
  "author": "Antigravity AI",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Dev10-sys/headlamp-strimzi-insight.git"
  },
  "homepage": "https://github.com/Dev10-sys/headlamp-strimzi-insight#readme",
  "dependencies": {
    "@kinvolk/headlamp-plugin": "^0.7.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "eslint": "^8.2.0",
    "prettier": "^2.4.1",
    "typescript": "^4.5.2"
  },
  "files": [
    "dist"
  ],
  "headlamp": {
    "compatibility": ">=0.7.0"
  }
